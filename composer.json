{
    "name": "digital-swing/wp-dev-tools",
    "description": "Digital Swing agency WP development tools",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lucas Demea",
            "email": "lucas@digital-swing.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "codeception/module-asserts": "1.3.1",
        "codeception/module-phpbrowser": "1.0.2",
        "codeception/module-webdriver": "1.4.0",
        "codeception/module-db": "1.1.0",
        "codeception/module-filesystem": "1.0.3",
        "codeception/module-cli": "1.1.1",
        "codeception/util-universalframework": "1.0.0",
        "squizlabs/php_codesniffer": "3.7.1",
        "roave/security-advisories": "dev-master",
        "szepeviktor/phpstan-wordpress": "1.0.2",
        "phpmd/phpmd": "2.11.1",
        "sebastian/phpcpd": "6.0.3",
        "php-parallel-lint/php-parallel-lint": "1.3.1",
        "phpro/grumphp": "1.13.0",
        "dealerdirect/phpcodesniffer-composer-installer": "0.7.2",
        "php-parallel-lint/php-console-highlighter": "0.5",
        "automattic/phpcs-neutron-ruleset": "3.4.0",
        "phpstan/extension-installer": "1.1.0",
        "phpstan/phpstan": "1.8.2",
        "ericmartel/codeception-email-mailhog": "1.0.8",
        "lucatume/wp-browser": "3.0.17",
        "php-stubs/wordpress-globals": "0.2.0",
        "wp-cli/wp-cli-bundle": "2.6.0",
        "cweagans/composer-patches": "1.7.2",
        "paulthewalton/acf-stubs": "^5.8"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "github-oauth": {},
        "secure-http": false,
        "allow-plugins": {}
    },
    "prefer-stable": true,
    "extra": {
        "grumphp": {
            "disable-plugin": true
        },
        "patches": {
            "php-stubs/wordpress-stubs": {
                "disable stub conflicting with roots/wp-password-bcrypt": "./patches/stubs.patch"
            }
        }
    }
}
